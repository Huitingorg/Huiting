using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace BDSoft.Components
{
    /// <summary>
    /// 组件描述
    /// </summary>
    public class ComponentDescriptionAttribute:Attribute
    {
        string name;

        /// <summary>
        /// 名字
        /// </summary>
        public string Name
        {
            get { return name; }
            set { name = value; }
        }

        string descrption;

        public string Descrption
        {
            get { return descrption; }
            set { descrption = value; }
        }


    }
}
